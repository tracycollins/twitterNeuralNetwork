{
  "apps": [
    {
      "name": "tnn",
      "script": "twitterNeuralNetwork.js",
      "cwd": "/home/tc/twitterNeuralNetwork",
      "node_args": "--max-old-space-size=24576, --experimental-json-modules",
      "exp_backoff_restart_delay": 100,
      "env": {
        "FORCE_COLOR": 1,
        "NODE_ENV": "production",
        "TNN_QUIT_ON_COMPLETE": false,
        "TNN_SERVER_MODE": true,
        "TNN_EVOLVE_TIMEOUT_DURATION": 600000,
        "TNN_KEEPALIVE_INTERVAL": 15000,
        "TNN_STATS_UPDATE_INTERVAL": 60000,
        "TNN_LOAD_TRAINING_SET_FROM_FILE": false,
        "TNN_CREATE_TRAINING_SET": true,
        "TNN_CREATE_TRAINING_SET_ONLY": true,
        "TNN_LOAD_ALL_INPUTS": false,
        "TNN_EVOLVE_ITERATIONS": 200,
        "NNB_MAX_INSTANCES": 5
      }
    }
  ]
}